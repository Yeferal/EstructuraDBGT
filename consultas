CREATE TABLE tabla1 ( col1 Int, col2 Double,col3 String,col4 Char );


INSERT INTO tabla1 ( col1,col2,col3,col4 ) VALUES ( 1,2,3,a );

INSERT INTO tabla1 ( col2,col3 ) VALUES ( 2,3 );

INSERT INTO tabla1 ( col2 ) VALUES ( 2 );

